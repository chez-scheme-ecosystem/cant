(to (main)
  (let pid (spawn (given ()
                    (report "hey")
                    (report "dud")
                    (! (me) 'not-this)
                    (? ('not-this (report "not this")))
                    (? (msg (report ['got msg])))
                    (report "dude")
                    (report (factorial 15)))))
  (report ['pid pid])
  (! pid 'yoohoo)
  (report (factorial 10)))

(to (report x)
  (print [(me) x]))

(to (factorial n)
  (match n
    (0 1)
    (_ (let x (factorial (- n 1)))
       (* n x))))

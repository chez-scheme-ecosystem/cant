#!/bin/bash
set -euo pipefail; shopt -s failglob  # 'Bash strict mode'

# Benchmark runner. Yes, it's absurdly crude.

echo "Reminder:"
echo "Run on unloaded system -- no web browser."
echo "Plug in the power."
echo ""
date
echo ""
echo euler:;   ./t euler
echo ""
echo nqueens:; ./t nqueens
echo ""
echo quick:;   ./t quick
